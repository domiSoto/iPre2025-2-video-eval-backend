{
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.0",
    "pptx-parser": "^1.1.7-beta.9",
    "pptx2json": "^0.0.10",
    "react-icons": "^5.5.0",
    "sequelize": "^6.32.1",
    "undici": "^7.16.0"
  },
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "start:dev": "NODE_ENV=development node server.js",
    "migrate": "node migrations/run_migrations.js",
    "sequelize": "npx sequelize-cli",
    "sequelize:create": "npx sequelize-cli db:create --config config/config.cjs",
    "sequelize:migrate": "npx sequelize-cli db:migrate --migrations-path sequelize_migrations --config config/config.cjs",
    "sequelize:undo": "npx sequelize-cli db:migrate:undo --migrations-path sequelize_migrations --config config/config.cjs"
  }
}
